class Student
{
    int marks;
    String name;
    Student(int marks,String name)
    {
        this.marks=marks;
        this.name=name;
    }
}
class Studentmarks
{
    int getMarks(Student s)
    {
        return s.marks;
    }
}
class Studentname
{
    String getName(Student s)
    {
        return s.name;
    }
}
public class Main
{
    public static void main (String[] args) 
    {
        Student s=new Student(95,"samrudh");
        Studentmarks sm=new Studentmarks();
        System.out.println(sm.getMarks(s));
        Studentname sn=new Studentname();
        System.out.println(sn.getName(s));
        
    }
}
